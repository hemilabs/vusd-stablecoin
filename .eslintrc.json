{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "jsdoc/require-jsdoc": "off",
    "max-len": ["error", {"code": 120}],
    "semi": ["error", "always"],
    "quotes": ["error", "double"]
  }
}
